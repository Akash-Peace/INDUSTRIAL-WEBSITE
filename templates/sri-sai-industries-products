	<script>
		function sendEmail() {
		    event.preventDefault();
			Email.send({
				SecureToken : "02d39e4a-2c53-4feb-9341-4c4ccb2f0559",
				To : 'srisaiindustries68@gmail.com',
				From : 'ssiwebsite68@gmail.com',
				Subject : "SSI-WEBSITE",
				Body : "Number: <br>"+ document.getElementById("exampleInputnumber").value + "<br>E-mail: <br>"+ document.getElementById("exampleInputEmail").value + "<br>Content: <br>" + document.getElementById("exampleInputcontent").value,
				}).then(function(response){
				  if (response == 'OK'){
					alert("Your query sent successfully.");
				  } else {
					alert("Unable to send your query.");
					console.log(response);
				  }
				});
		}
	</script>
    <script>
        var i = document.getElementsByClassName("icon");
        var ic = document.getElementsByClassName("iconclose");
        function myFunction(z) {
		  if (z.matches) {
			i[0].style.display = "none";
			ic[0].style.display = "none";
		  }
		  else{
			i[0].style.display = "block";
		  }
		}
        var z = window.matchMedia("(min-width: 601px)")
		myFunction(z)
		z.addListener(myFunction)
    	var value_for_ddm_toggle = 0;
    	function respmenu() {
		  var x = document.getElementById("myTopnav");
		  if (x.className === "topnav") {
			x.className += " responsive";
			$('.topnav a.icon').css('display','none');
			$('.topnav a.iconclose').css('display','block');
			//document.getElementsByClassName("icon")[0].style.display = "none";
		    //document.getElementsByClassName("iconclose")[0].style.display = "block";
		  } else {
			x.className = "topnav";
			$('.topnav a.icon').css('display','block');
			$('.topnav a.iconclose').css('display','none');
			//document.getElementsByClassName("iconclose")[0].style.display = "none";
		    //document.getElementsByClassName("icon")[0].style.display = "block";
			if (value_for_ddm_toggle == 1){
				document.getElementById("ddm-content").classList.toggle("showmenu");
				value_for_ddm_toggle = 0;
			}
		  }
		}
	</script>
    <script>
		window.onscroll = function() {scrollFunction()};

		function scrollFunction() {
		  if (document.body.scrollTop > 775 || document.documentElement.scrollTop > 775) {
			document.getElementById("navbar").style.top = "0";
		  } else {
			document.getElementById("navbar").style.top = "-50px";
		  }
		}
	</script>
