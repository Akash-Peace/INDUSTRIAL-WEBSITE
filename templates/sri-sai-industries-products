		window.onclick = function(event) {
		  if (event.target == infoovmodal) {
			infoovmodal.style.display = "none";
		  }
		  if (event.target == infomodal) {
			infomodal.style.display = "none";
		  }
		}
	</script>
    <script>
		function showmenu() {
		  if (value_for_ddm_toggle == 0){
		  	value_for_ddm_toggle = 1;
		  } else {
		  	value_for_ddm_toggle = 0;
		  }
		  document.getElementById("ddm-content").classList.toggle("showmenu");
		}
	</script>
    <script src="https://unpkg.com/aos@2.3.0/dist/aos.js"></script>
    <script>
        AOS.init({
          duration: 1500,
        })
    </script> 
  </body>
